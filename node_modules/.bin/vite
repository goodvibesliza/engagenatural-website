#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Projects/Websites/engagenatural-website/node_modules/.pnpm/vite@6.3.5_@types+node@24.0_53cee26d6baeab47f5ed353258cea7a7/node_modules/vite/bin/node_modules:/mnt/c/Projects/Websites/engagenatural-website/node_modules/.pnpm/vite@6.3.5_@types+node@24.0_53cee26d6baeab47f5ed353258cea7a7/node_modules/vite/node_modules:/mnt/c/Projects/Websites/engagenatural-website/node_modules/.pnpm/vite@6.3.5_@types+node@24.0_53cee26d6baeab47f5ed353258cea7a7/node_modules:/mnt/c/Projects/Websites/engagenatural-website/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Projects/Websites/engagenatural-website/node_modules/.pnpm/vite@6.3.5_@types+node@24.0_53cee26d6baeab47f5ed353258cea7a7/node_modules/vite/bin/node_modules:/mnt/c/Projects/Websites/engagenatural-website/node_modules/.pnpm/vite@6.3.5_@types+node@24.0_53cee26d6baeab47f5ed353258cea7a7/node_modules/vite/node_modules:/mnt/c/Projects/Websites/engagenatural-website/node_modules/.pnpm/vite@6.3.5_@types+node@24.0_53cee26d6baeab47f5ed353258cea7a7/node_modules:/mnt/c/Projects/Websites/engagenatural-website/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
